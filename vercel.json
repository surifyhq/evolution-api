{
  "version": 2,
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "evolution-api",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
